<text>pages/page4/page4.wxml</text>
<view class="upper-card">
  <view class="left-images">
    <image class="profile" src="{{profile}}" mode="widthFix"></image>
    <image class="gender" src="{{gender}}" mode="widthFix"></image>
  </view>
  <view class="right-texts">
    <view>
      <view class="name">{{name}}</view>
      <view class="campus"></view>
    </view>

    <view class="info"></view>
  </view>
</view>
<view class="lower-buttons">
  <button>上传证件照</button>
  <button>信息修改</button>
  <button>干事登录</button>
</view>